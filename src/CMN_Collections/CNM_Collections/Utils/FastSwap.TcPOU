*)

FUNCTION FastSwap
VAR_INPUT
	object1 :POINTER TO __XWORD;
	object2 :POINTER TO __XWORD;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[RETURN(object1 = object2);
RETURN(object1 = 0 OR object2 = 0);
object1^ := object2^ XOR object1^;
object2^ := object2^ XOR object1^;
object1^ := object2^ XOR object1^;
]]></ST>
    </Implementation>
